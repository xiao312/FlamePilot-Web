{
  "name": "gemini-cli-web",
  "version": "1.5.0",
  "description": "A stylish web-based UI for Gemini CLI",
  "type": "module",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently --kill-others \"npm run server\" \"npm run client\"",
    "dev-mock": "concurrently --kill-others \"npm run mock-server\" \"npm run client\"",
    "server": "node server/index.js",
    "mock-server": "GEMINI_PATH=./bin/gemini node server/index.js",
    "client": "vite --host",
    "build": "vite build",
    "preview": "vite preview",
    "start": "npm run build && npm run server"
  },
  "keywords": [
    "gemini",
    "gemini-cli",
    "gemini-cli-web",
    "gui",
    "web",
    "ai",
    "code",
    "ui",
    "assistant"
  ],
  "author": "ssdeanx",
  "homepage": "https://github.com/ssdeanx/gemini-cli-web",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ssdeanx/gemini-cli-web.git"
  },
  "bugs": {
    "url": "https://github.com/ssdeanx/gemini-cli-web/issues"
  },
